profile-manager-description = { -brand-short-name } jiusu profilā globoj īstatiejumus, izvieles i cytu lītuotuoja informaceju.

profile-manager-work-offline =
    .label = Struoduot bezsaistē
    .accesskey = b

profile-manager-use-selected =
    .label = Izmantuot izavalātū profilu, navaicojūt īluodes laikā
    .accesskey = s
