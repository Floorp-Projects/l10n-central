profile-selection-window =
    .title = { -brand-short-name } - izavielit lītuotuoja profilu

profile-selection-button-accept =
    .label = Suokt { -brand-short-name }

profile-selection-button-cancel =
    .label = Izīt

profile-selection-new-button =
    .label = Izveiduot profilu...
    .accesskey = I

profile-selection-rename-button =
    .label = Puodieveit profilu...
    .accesskey = r

profile-selection-delete-button =
    .label = Dzēst profilu...
    .accesskey = D

