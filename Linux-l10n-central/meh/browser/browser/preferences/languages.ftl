webpage-languages-window =
    .title = Ke´i tu´un nuu página web
    .style = width: 40em

languages-close-key =
    .key = w

languages-description = Iyo páginas web vaji ji kue´e tu´un. Kaji da tu´un kutuvi xinañu´u nuu páginas web

languages-customize-spoof-english =
    .label = Taji versiones ji inglés páginas web kue´eka privacidad

languages-customize-moveup =
    .label = Kaka si̱kɨ̱
    .accesskey = K

languages-customize-movedown =
    .label = Kaka chuve
    .accesskey = K

languages-customize-remove =
    .label = Xita
    .accesskey = X

languages-customize-select-language =
    .placeholder = Kaji iin tu´un tee...

languages-customize-add =
    .label = Tee
    .accesskey = T

# The pattern used to generate strings presented to the user in the
# locale selection list.
#
# Example:
#   Icelandic [is]
#   Spanish (Chile) [es-CL]
#
# Variables:
#   $locale (String) - A name of the locale (for example: "Icelandic", "Spanish (Chile)")
#   $code (String) - Locale code of the locale (for example: "is", "es-CL")
languages-code-format =
    .label = { $locale }  [{ $code }]

languages-active-code-format =
    .value = { languages-code-format.label }

browser-languages-window =
    .title = { -brand-short-name } ke´i tu´un
    .style = width: 40em

browser-languages-search = Nánuku kue´e tu´un...

browser-languages-searching =
    .label = Nánuku tu´un...

browser-languages-downloading =
    .label = Xinuu...

browser-languages-select-language =
    .label = Kaji iin tu´un tee...
    .placeholder = Kaji iin tu´un tee...

browser-languages-installed-label = Tu´un nchu´un
browser-languages-available-label = Tu´un íyo

browser-languages-error = { -brand-short-name } ntu nkuvi xinuun da tu´un ntañu´u. Kune´ya íyo internet a xíín nachu´un tuku.
