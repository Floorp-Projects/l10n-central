colors-window =
    .title = Գոյներ
    .style =
        { PLATFORM() ->
            [macos] width: 41em
           *[other] width: 38em
        }

colors-close-key =
    .key = w

colors-page-override = Վերագրել էջի կողմից հատկորոշուած գոյները Ձեր ընտրածով
    .accesskey = O

colors-page-override-option-always =
    .label = Միշտ
colors-page-override-option-auto =
    .label = Միայն Բարձր ցայտունութեան ձեւաւորումների համար
colors-page-override-option-never =
    .label = Երբեք

colors-text-and-background = Գրոյթ եւ խորապատկեր

colors-text-header = Գրոյթ
    .accesskey = Գ

colors-background = Խորապատկեր
    .accesskey = Խ

colors-use-system =
    .label = Աւգտագործել համակարգի գոյները
    .accesskey = s

colors-underline-links =
    .label = Յղումներն ընդգծել
    .accesskey = U

colors-links-header = Յղումների գոյներ

colors-unvisited-links = Չայցելած յղումներ
    .accesskey = L

colors-visited-links = Այցելած յղումներ
    .accesskey = V
